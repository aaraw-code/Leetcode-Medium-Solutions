# This implementation is based on Bubble Sort.
# Although it produces correct results, it exceeds the time limit (TLE) on LeetCode 912 

class Solution:
    def sortArray(self, nums: List[int]) -> List[int]:

        n = len(nums)
        
        for i in range(n):
            for j in range(n-i-1):
                if nums[j] > nums[j+1]:
                    #swaping
                    temp = nums[j]
                    nums[j] = nums[j+1]
                    nums[j+1] = temp
        return nums
