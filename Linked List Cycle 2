
class Solution:
    def detectCycle(self, head: Optional[ListNode]) -> Optional[ListNode]:
        slow = head
        fast = head
        
        cycle = False
        while fast!=None and fast.next!=None:
            slow = slow.next
            fast = fast.next.next

            if slow==fast:
                cycle = True
                break
        if not cycle:
            return None

        l = 0
        while slow.next!=fast:
            slow = slow.next
            l+=1
        l+=1
        slow = slow.next

        slow = head
        fast = head

        for i in range(l):
            fast = fast.next
        while slow!=fast:
            slow = slow.next
            fast = fast.next
        return slow
